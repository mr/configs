URxvtFuzzy*internalBorder: 30
URxvt.scrollBar: false
xterm*scrollBar: false
URxvt.perl-ext-common: default,matcher,copy-paste
URxvt.urlLauncher:      /usr/bin/firefox
URxvt.matcher.button:   1
URxvt.keysym.F11: perl:fullscreen:switch
URxvt.tabbed.tabbar-fg: -2
URxvt.tabbed.tabbar-bg: -1
URxvt.tabbed.tab-fg:    -2
URxvt.tabbed.tab-bg:    4
URxvt.font: xft:Source Code Pro for Powerline:size=12
URxvt.boldFont: xft:Source Code Pro for Powerline:size=12

XTerm*renderFont: true
XTerm*faceName: cure-powerline
XTerm*faceSize: 8.6
XTerm*allowBoldFonts: false

! Base16 Monokai
